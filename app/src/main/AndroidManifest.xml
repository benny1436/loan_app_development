<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nubankdesigntest">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/quartalogo"
        android:label="Quarta."
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NubankDesignTest">

        <activity android:name=".LoanDetails" />
        <activity android:name=".PaymentHistory" />
        <activity android:name=".LoanCalcutor" />
        <activity android:name=".ApplyLoan" />
        <activity android:name=".Registration" />
        <activity android:name=".MoneyTalk" />
        <activity android:name=".Profile" />
        <activity android:name=".HomeDashboard" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>